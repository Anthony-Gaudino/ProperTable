    "es6-set": "^0.1.4",
    "fixed-data-table": "^0.6.0",
    "immutable": "^3.7.6",
    "moment": "^2.10.6",
    "numeral": "^1.5.3",
    "react": "^0.14.7",
    "react-dimensions": "^1.0.2",
